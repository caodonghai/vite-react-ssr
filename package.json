{
  "name": "vite-react-ssr",
  "version": "0.0.1",
  "description": "React SSR With Vite, Koa and React-RouterV6",
  "keywords": [
    "SSR",
    "react",
    "react-router",
    "vite",
    "koa"
  ],
  "scripts": {
    "dev": "nodemon --watch server.ts --exec 'ts-node server.ts'",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "serve": "npm run build && cross-env NODE_ENV=production ts-node server.ts",
    "prerender": "vite build --outDir dist/static && npm run build:server && ts-node prerender"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-connect": "^2.1.0",
    "koa-static": "^5.0.0",
    "nodemon": "^2.0.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-static": "^4.0.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^1.3.0",
    "colors": "^1.4.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "author": "wqhui",
  "license": "MIT"
}
